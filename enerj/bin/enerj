#!/bin/sh

jsr308jar=$JSR308/checkers/binary/jsr308-all.jar
enerjdir=`dirname $0`/..
enerjjar=$enerjdir/build/enerj.jar
cfrtjar=$enerjdir/../checker-runtime/build/cfrt.jar
plumejar=$enerjdir/plume.jar
commonjar=/dcsdata/home/hdokani2/Desktop/cs527/commons-math3-3.3.jar
junitjar=/dcsdata/home/hdokani2/.m2/repository/junit/junit/4.11/junit-4.11.jar
jargs=
for arg
do
    case "$arg" in
    -noisy) jargs="$jargs -DPrecisionRuntime=enerj.rt.PrecisionRuntimeNoisy" ;;
    *) jargs="$jargs $arg" ;;
      # echo $jargs;;
    esac
done

command="java -DPrecisionRuntime=enerj.rt.PrecisionRuntimeNoisy  -Xbootclasspath/a:$jsr308jar:$cfrtjar:$enerjjar:$plumejar:$commonjar:$junitjar:. org.junit.runner.JUnitCore  $jargs"
#command="java -Xbootclasspath/a:$jsr308jar:$cfrtjar:$enerjjar:$plumejar:$commonjar:. $jargs"
#command="java -Xbootclasspath/a:$jsr308jar:$cfrtjar:$enerjjar:$plumejar $jargs"
#command="java -Xbootclasspath/a:$jsr308jar:$cfrtjar:$enerjjar:$plumejar:target/classes:target/test-classes:/dcsdata/home/hdokani2/.m2/repository/junit/junit/4.11/junit-4.11.jar:/dcsdata/home/hdokani2/.m2/repository/org/hamcrest/hamcrest-core/1.3/hamcrest-core-1.3.jar org.junit.runner.JUnitCore $jargs"
exec $command
